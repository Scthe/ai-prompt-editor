  "main": "src/index.tsx",
  "scripts": {
    "prestart": "yarn clean",
    "start": "node esbuild-script.js --dev",
    "prebuild": "yarn clean",
    "build": "node esbuild-script.js",
    "preview": "yarn build && http-server build",
    "clean": "rimraf ./build/* || true",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js,.tsx,.ts src",
    "lark:compile": "lark-js 'src/lib/lark/prompt_grammar.lark' -o 'src/lib/lark/prompt_grammar_lark.js'",
    "ts-dry": "yarn tsc",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "classnames": "^2.5.1",
    "debounce": "^2.0.0",
    "exifreader": "^4.21.0",
    "framer-motion": "^11.0.3",
    "localforage": "^1.10.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.21.3",
    "react-simple-code-editor": "^0.13.1",
    "use-debounce": "^10.0.0",
    "zustand": "^4.5.0"
